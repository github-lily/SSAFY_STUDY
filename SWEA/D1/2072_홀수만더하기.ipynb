   "source": [
    "# 홀수만 더하기"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "T = int(input())\n",
    "for tc in range(1,T+1) :\n",
    "    arr = list(map(int,input().split()))\n",
    "\n",
    "    sum = 0\n",
    "    for i in range(len(arr)) :\n",
    "        if arr[i] % 2 == 1 : #홀수일 때\n",
    "           sum += arr[i]\n",
    "        \n",
    "    print(f'#{tc} {sum}')\n",
    "    "
   ]
  }
 ],
