   "source": [
    "from collections import deque\n",
    "\n",
    "N, K = map(int,input().split())\n",
    "\n",
    "q = deque(range(1,N+1))\n",
    "result = []\n",
    "\n",
    "while q:\n",
    "    for _ in range(K-1) :\n",
    "        q.append(q.popleft())\n",
    "    result.append(q.popleft())\n",
    "\n",
    "\n",
    "ans = '<{}>'.format(\", \".join(map(str,result)))\n",
    "\n",
    "print(ans)\n",
    "\n"
   ]
  }
 ],
